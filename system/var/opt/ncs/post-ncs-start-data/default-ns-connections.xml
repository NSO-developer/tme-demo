  <default-ns-topology-connections>
    <nsd>hosted-corporate-service</nsd>
    <sap-topology-connection>
      <sapd>dci-1</sapd>
      <device>dci0</device>
      <ios-xr-TenGigE>0/3/0/1</ios-xr-TenGigE>
    </sap-topology-connection>
    <sap-topology-connection>
