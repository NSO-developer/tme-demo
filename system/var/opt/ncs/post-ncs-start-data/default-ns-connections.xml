      <sapd>a</sapd>
      <device>p2</device>
      <ios-xr-TenGigE>0/3/0/4</ios-xr-TenGigE>
    </sap-topology-connection>
    <sap-topology-connection>
      <sapd>z</sapd>
      <device>ce7</device>
      <ios-GigabitEthernet>0/2</ios-GigabitEthernet>
    </sap-topology-connection>
  </default-ns-topology-connections>
  </tme-demo>
